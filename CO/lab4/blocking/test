T_3 ;
    %wait E_01037848;
    %load/v 8, v01034630_0, 3;
    %set/v v0103E670_0, 8, 3;
    %jmp T_3;
    .thread T_3;
    .scope S_01038C38;
T_4 ;
    %wait E_01037848;
    %load/v 8, v0103E670_0, 3;
    %set/v v01034580_0, 8, 3;
    %jmp T_4;
    .thread T_4;
    .scope S_01038BB0;
T_5 ;
    %delay 5000, 0;
    %load/v 8, v0103E720_0, 1;
    %inv 8, 1;
    %set/v v0103E720_0, 8, 1;
    %jmp T_5;
    .thread T_5;
    .scope S_01038BB0;
T_6 ;
    %vpi_call 2 16 "$dumpfile", "blocking.vcd";
    %vpi_call 2 17 "$dumpvars";
    %set/v v0103E720_0, 0, 1;
    %movi 8, 5, 3;
    %set/v v0103E778_0, 8, 3;
    %delay 100000, 0;
