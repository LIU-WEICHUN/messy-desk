v01034630_0 .var "nblk_reg2", 2 0;
v0103E670_0 .var "nblk_reg3", 2 0;
E_01037848 .event posedge, v01033A98_0;
    .scope S_01038C38;
T_0 ;
    %wait E_01037848;
    %load/v 8, v01033AF0_0, 3;
    %set/v v01073C18_0, 8, 3;
    %load/v 8, v01073C18_0, 3;
    %set/v v01073B50_0, 8, 3;
    %load/v 8, v01073B50_0, 3;
    %set/v v01033A40_0, 8, 3;
    %load/v 8, v01033A40_0, 3;
    %set/v v010739F8_0, 8, 3;
    %jmp T_0;
    .thread T_0;
    .scope S_01038C38;
T_1 ;
    %wait E_01037848;
    %load/v 8, v01033AF0_0, 3;
    %set/v v010345D8_0, 8, 3;
    %jmp T_1;
    .thread T_1;
    .scope S_01038C38;
T_2 ;
    %wait E_01037848;
    %load/v 8, v010345D8_0, 3;
    %set/v v01034630_0, 8, 3;
    %jmp T_2;
