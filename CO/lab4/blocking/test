v0103E6C8_0 .net "blk", 2 0, v010739F8_0; 1 drivers
v0103E720_0 .var "clk", 0 0;
v0103E778_0 .var "in", 2 0;
v0103E7D0_0 .net "nblk", 2 0, v01034580_0; 1 drivers
S_01038C38 .scope module, "test" "blocking" 2 10, 3 1, S_01038BB0;
